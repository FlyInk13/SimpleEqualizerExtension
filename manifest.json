{
    "name": "SimpleEqualizerExtension",
    "version": "1.0",
    "short_name": "SimpleEQ",
    "description": "Simple Chrome Audio Equalizer Extension",
    "permissions": ["tabCapture", "activeTab"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icon.png"
    },
    "icons": {
        "128": "icon.png"
    },
    "manifest_version": 2
}
